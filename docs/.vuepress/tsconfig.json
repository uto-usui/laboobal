{
  "compilerOptions": {
    "target": "es2019",
    "module": "esnext",
    "strict": true,
    "resolveJsonModule": true,
    "downlevelIteration": true,
    "allowJs": true,
    "noEmit": true,
    "noImplicitAny": false,
    "importHelpers": true,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "baseUrl": ".",
    "types": [ "vuepress-types" ],
    "paths": {
      "@/*": [
        "./*"
      ],
      "@assets/*": [
        "./assets/*"
      ]
    },
    "lib": [
      "esnext",
      "dom",
      "dom.iterable",
      "scripthost"
    ]
  }
}
